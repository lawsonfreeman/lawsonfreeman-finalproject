{% extends '_templates/base.njk' %}

{% block page_title %}Discography{% endblock %}

{% block page_description %}The index search description for this page goes here.{% endblock %}

{% block content %}
<div class="container">
  <div class="heading"><h1>Queen discography</h1></div>
  <div class="row">
    {% for discography in queen.discography %}
        <div class="col-sm-6 col-md-4 col-lg-3">
        <img src="img/{{ discography.image }}" alt="" class="img-fluid">
        <h3>{{ discography.title }}</h3>
        <h5>{{ discography.year }}</h5>
      </div>
    {% endfor %}
  </div>
{% endblock %}
  </div>
